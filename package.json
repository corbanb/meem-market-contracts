{
  "name": "@zoralabs/auction-house",
  "version": "1.1.3",
  "private": false,
  "homepage": "https://github.com/ourzora/coldies",
  "repository": {
    "type": "git",
    "url": "https://github.com/ourzora/coldies.git"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist/**/*",
    "./dist/*"
  ],
  "scripts": {
    "prepublishOnly": "yarn build:contracts && yarn build:package",
    "build:contracts": "hardhat compile",
    "build:package": "rm -rf ./dist && tsc && cp typechain/*.d.ts dist/typechain && cp -R addresses dist && cp -R artifacts/contracts dist/artifacts && cp -R contracts dist",
    "test": "hardhat test",
    "verify": "hardhat verify",
    "lint": "eslint --max-warnings=0 --ext=.ts,.tsx . && tsc -p . --noEmit",
    "lint:fix": "eslint --fix --ext=.ts,.tsx .",
    "release": "semantic-release",
    "size": "hardhat size-contracts",
    "deploy": "hardhat deploy",
    "upgradeContract": "hardhat upgradeContract",
    "network": "hardhat node",
    "console": "hardhat console",
    "watch": "concurrently -r --kill-others \"chokidar 'contracts/**/*.sol' -c 'yarn build:contracts'\""
  },
  "devDependencies": {
    "@float-capital/solidity-coverage": "^0.7.17",
    "@kengoldfarb/semantic-release-config": "^1.0.3",
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-etherscan": "^2.1.1",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@openzeppelin/contracts-upgradeable": "^4.4.0",
    "@openzeppelin/hardhat-upgrades": "^1.12.0",
    "@typechain/ethers-v5": "^6.0.4",
    "@types/chai": "^4.2.15",
    "@types/chai-as-promised": "^7.1.3",
    "@types/mocha": "^8.2.1",
    "@types/node": "^14.14.35",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^6.4.0",
    "eslint": "^7.32.0",
    "eslint-config-kengoldfarb": "^1.3.0",
    "ethereum-waffle": "^3.3.0",
    "ethers": "^5.0.32",
    "hardhat": "^2.1.1",
    "hardhat-abi-exporter": "^2.3.1",
    "hardhat-contract-sizer": "^2.1.1",
    "hardhat-gas-reporter": "^1.0.4",
    "hardhat-typechain": "^0.3.5",
    "prettier": "^2.5.0",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "ts-generator": "^0.1.1",
    "ts-node": "^9.1.1",
    "typechain": "^4.0.3",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.4.0",
    "dotenv": "^10.0.0",
    "fs-extra": "^9.1.0",
    "minimist": "^1.2.5",
    "solidity-coverage": "^0.7.16"
  }
}